{
  "title": "实验手册",
  "description" : "信息科学基础课程实验手册",
  "author": "王顶",
  "language": "zh-hans",
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "-sharing",
    "books",
    "edit-link",
    "sharing-plus",
    "github-buttons@2.1.0"
  ],
  "pluginsConfig": {
    "books": {
      "prism_themes": "prismjs/themes/prism.css",
      "github_url": "https://github.com/wangding/info-theory-lab-manual"
    },
    "theme-default": {
      "showLevel": false
    },
    "github-buttons": {
      "repo": "wangding/info-theory-lab-manual",
      "types": ["star"],
      "size": "small"
    },
    "edit-link": {
      "base": "https://github.com/wangding/info-theory-lab-manual/blob/master/",
      "label": "编辑本页"
    },
    "sharing": {
      "douban": true,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": true,
      "qzone": true,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "weibo", "douban", "qzone", "qq"
      ]
    }
  }
}
